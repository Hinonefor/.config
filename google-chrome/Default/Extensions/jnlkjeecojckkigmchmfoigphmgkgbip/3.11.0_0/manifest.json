   "key": "",
   "manifest_version": 2,
   "name": "__MSG_appName__",
   "options_ui": {
      "chrome_style": false,
      "open_in_tab": true,
      "page": "option/index.html"
   },
   "permissions": [ "webRequest", "webRequestBlocking", "downloads", "http://*/*", "https://*/*" ],
